[CreateAssetMenu(fileName = "New Asset", menuName = "Sword")]

public class Sword : ScriptableObject 
{
  public new string name;
  public string description;

  public Sprite artwork;

  public int attack;
  public int requiredLv;


  public void Print()
  {
    Debug.Log(name + ": " + description + "Required Level: " + requiredLv);

  }

}
